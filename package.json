{
  "name": "uni-orm",
  "version": "1.0.0",
  "description": "Universal ORM that works across all programming languages and databases",
  "main": "src/index.js",
  "bin": {
    "uni-orm": "./bin/cli.js",
    "uniorm": "./bin/cli.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "cli": "node bin/cli.js",
    "dashboard": "node src/dashboard/server.js",
    "dev:engine": "node engine/server.js",
    "dev:dashboard": "node dashboard/server.js",
    "test": "jest",
    "build": "npm run build:cli && npm run build:dashboard",
    "build:cli": "pkg bin/cli.js --out-path dist/",
    "build:dashboard": "cd src/dashboard && npm run build",
    "build:bin": "pkg ./bin/cli.js --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output dist/uniorm"
  },
  "keywords": [
    "orm",
    "database",
    "universal",
    "migration",
    "multi-language",
    "prisma",
    "sequelize",
    "typeorm"
  ],
  "author": "UniORM Team",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.12",
    "mongodb": "^5.9.2",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "semver": "^7.5.4",
    "sql.js": "^1.13.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
